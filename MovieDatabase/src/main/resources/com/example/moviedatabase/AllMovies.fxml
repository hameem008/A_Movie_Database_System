      <TableView fx:id="tableView" layoutX="67.0" layoutY="146.0" prefHeight="430.0" prefWidth="782.0" stylesheets="@AllMovies.css">
         <columns>
            <TableColumn fx:id="MovieName" prefWidth="89.5999984741211" text="Movie Name" />
