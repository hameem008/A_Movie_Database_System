            <TableColumn fx:id="Genre1" prefWidth="75.0" text="Genre 1" />
            <TableColumn fx:id="Genre2" prefWidth="75.0" text="Genre 2" />
            <TableColumn fx:id="Genre3" prefWidth="75.0" text="Genre 3" />
            <TableColumn fx:id="RunningTime" minWidth="0.0" prefWidth="115.20010375976562" text="Running Time" />
