            <TableColumn fx:id="Revenue" prefWidth="115.1998291015625" text="Revenue" />
         </columns>
      </TableView>
      <Label fx:id="CompanyName" alignment="CENTER" layoutX="128.0" layoutY="64.0" prefHeight="40.0" prefWidth="660.0" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font name="Bank Gothic Light BT" size="30.0" />
         </font>
      </Label>
      <Label layoutX="400.0" layoutY="109.0" text="All Movies" textFill="WHITE">
         <font>
            <Font name="Bank Gothic Light BT" size="20.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
