<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="900.0" styleClass="background" stylesheets="@TotalProfit.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.moviedatabase.TotalProfit">
   <children>
      <Button layoutX="40.0" layoutY="30.0" mnemonicParsing="false" onAction="#clickOnBack" styleClass="back" stylesheets="@TotalProfit.css" text="Back" textFill="WHITE">
         <font>
            <Font name="Bank Gothic Light BT" size="20.0" />
         </font>
      </Button>
      <Label fx:id="TotalProfit" layoutX="253.0" layoutY="179.0" prefHeight="30.0" prefWidth="312.0" text="label" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font name="Bank Gothic Light BT" size="25.0" />
         </font>
      </Label>
      <Label layoutX="67.0" layoutY="181.0" text="Total profit:" textFill="WHITE">
         <font>
            <Font name="Bank Gothic Light BT" size="25.0" />
         </font>
      </Label>
      <Label fx:id="CompanyName" alignment="CENTER" layoutX="128.0" layoutY="64.0" prefHeight="40.0" prefWidth="660.0" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font name="Bank Gothic Light BT" size="30.0" />
         </font>
      </Label>
      <Label layoutX="483.0" layoutY="430.0" text="Total Movies: " textFill="WHITE">
         <font>
            <Font name="Bank Gothic Light BT" size="25.0" />
         </font>
      </Label>
      <Label fx:id="TotalMovies" layoutX="677.0" layoutY="430.0" prefHeight="27.0" prefWidth="204.0" text="Label" textFill="WHITE">
         <font>
            <Font name="Bank Gothic Light BT" size="25.0" />
         </font>
      </Label>
      <Button layoutX="750.0" layoutY="530.0" mnemonicParsing="false" onAction="#clickOnRefresh" styleClass="back" stylesheets="@TotalProfit.css" text="Refresh" textFill="WHITE">
         <font>
            <Font name="Bank Gothic Light BT" size="20.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
